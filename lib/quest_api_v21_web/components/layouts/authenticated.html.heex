      <div>
        <span class={"ml-4 w-6 h-6 hero-home#{if @live_action == :home, do: "-solid"}"}>
        </span>
        <p class={"text-center font-base #{if @live_action == :home, do: "font-bold"}"}>Home</p>
    </div>
    </.link>
      <!-- Quests -->
      <.link patch={"/quests"} replace={true} class={" #{if @live_action == :quests, do: "text-gray-900"} text-gray-800 py-2 w-14 h-14 transition-all ease-in-out text-xs"}>

