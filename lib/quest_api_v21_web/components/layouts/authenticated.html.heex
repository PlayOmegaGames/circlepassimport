    </div>
    </.link>
  <!-- Profile -->

    <.link patch={"/profile"} replace={true} class={" #{if @live_action == :profile, do: "text-gray-900"} text-gray-800 py-2 w-14 h-14 transition-all ease-in-out text-xs"}>

    <div>
      <span class={"ml-4 w-6 h-6 hero-user#{if @live_action == :profile, do: "-solid"}"}>
      </span>
      <p class={"text-center #{if @live_action == :profile, do: "font-bold", else: "font-base"}"}>Profile</p>
    </div>
    </.link>
  </div>
</div>
</div>
<main class={"min-h-screen #{ if @background_color == "false", do: "bg-gradient-to-t from-violet-300 to-violet-100", else: @background_color} md:px-8 sm:px-6 lg:px-8"}>
  <div class="mx-auto pb-40 max-w-2xl">
    <.flash_group flash={@flash} />
    <%= @inner_content %>
  </div>
</main>

